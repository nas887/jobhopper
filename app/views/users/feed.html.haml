:javascript
  $(function() {
      $( "#catalog li" ).draggable({
        appendTo: "body",
        helper: "clone"
      });
      $( "#cart ol" ).droppable({
        activeClass: "ui-state-default",
        hoverClass: "ui-state-hover",
        accept: ":not(.ui-sortable-helper)",
        drop: function( event, ui ) {
          var job_id = ui.draggable.attr('id');
          $( this ).find( ".placeholder" ).remove();
          $( "<li id=" + job_id + "></li>" ).text( ui.draggable.text() ).appendTo( this );
          $("#"+job_id).remove();
          $.ajax({
            method: "GET",
            url: '/add_to_queue/' + job_id
          }).success(function(response){
            console.log("oo shit mothafucka")
          });
        }
      }).sortable({
        items: "li:not(.placeholder)",
        sort: function() {
          $( this ).removeClass( "ui-state-default" );
        }
      });
  });
.feed
  %h2 Your feed
  %div{:id => "catalog"}
    %ul
    -i = 0
    -while i < @jobs[:miles].count
      %ul
        %li{:id => @jobs[:results][i].id, :class => "job"}= @jobs[:results][i].name
        %li= "Added #{time_ago_in_words(@jobs[:results][i].created_at)} ago"
        %li= "Approximately #{@jobs[:miles][i].to_i} miles away"
      - i += 1
.queue
  %div{:id => "cart"}
    %h1{:class => "ui-widget-header"} Job Queue
    %div{:class => "ui-widget-content"}
      %ol
        -if @queue == nil
          %li{:class => "placeholder"} Add you jobs here
        -else
          -@queue.each do |job|
            %li="#{job.name}" 
            
