.wizard_container
  .location
    %h3= @user.location
  .new_skills
    %h3 Pick locations you're interested in
    = form_tag search_locations_path, :class => "user_tags", :method => "get" do
      = text_field_tag :search, params[:search], :placeholder => "Search for Locations", :class => "wizard-search", :style => "border:none; width: 20%;"
  %ul.search_results
  .skills
    %ul.skills-list
    -@user_locs.each do |loc|
      %li
        %button=loc.name 
        %a.delete{:id => loc.id, :href => '#'} X 